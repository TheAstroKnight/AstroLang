class IfElse { // Create a class.
	function void Main() { // The required method for every Main class.
		if (5 < 6) { // Check if the condition 5 < 6 is true.
			System.print("The 'if'-statement returned true!"); // If the condition was true, execute this.
		} elseif (5 == 6) { // If the "if"-statement returned false, check this condition.
			System.print("The 'elseif'-statement returned true!"); // If the condition was true, execute this.
		} else { // If no conditions are returning true, execute the body.
			System.print("Neither the 'if'-statement, nor the 'elseif'-statement returned true!"); // If there were no conditions returning true, execute this.
		}
	}
}